<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>

  <groupId>io.apiman.plugins</groupId>
  <artifactId>apiman-plugin-registry</artifactId>
  <version>1.2.6-SNAPSHOT</version>
  <packaging>pom</packaging>
  <name>apiman-plugin-registry</name>

  <distributionManagement>
    <snapshotRepository>
      <id>phabricator</id>
      <name>phabricator-snapshots-deploy</name>
      <url>http://phabricator:8081/artifactory/libs-snapshot-local</url>
    </snapshotRepository>
    <repository>
      <id>phabricator</id>
      <name>phabricator-release-deploy</name>
      <url>http://phabricator:8081/artifactory/libs-release-local</url>
    </repository>
  </distributionManagement>

  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <maven.build.timestamp.format>yyyy-MM-dd HH:mm:ss</maven.build.timestamp.format>
  </properties>

</project>
